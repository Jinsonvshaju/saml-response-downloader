{
  "manifest_version": 3,
  "name": "SAML Response Downloader",
  "version": "1.0",
  "description": "An extension to write and save files.",
  "icons": {
    "16": "icon 16x16.png",
    "48": "icon 48x48.png",
    "128": "icon 128x128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["downloads", "activeTab", "downloads.shelf","webRequest","nativeMessaging","storage"],
  "host_permissions": [
    "<all_urls>"
  ]
}
